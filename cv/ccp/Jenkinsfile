pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo "Running ${BUILD_ID} on ${JENKINS_URL} ${params.CODE_BRANCH} ${params.CODE_VERSION} ${params.CODE_STRING}"
                sh '''
                   env
                   printenv
                '''
                sh 'ls -l'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
