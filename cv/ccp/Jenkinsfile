pipeline {
    agent any
    variables {
        CURRENT_BRANCH: 
    }

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
                sh '''
                   echo "Running ${BUILD_ID} on ${JENKINS_URL} ${CURRENT_BRANCH} ${CURRENT_VERSION}"
                '''
                sh 'ls -l'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
